package sheridan;
//declaration of class mathexpress
public class mathexpess
{
    //declaration of main method 
    public static void main(String args[])
    { // declaration and inisialization
        double volume,radius=2.2;
        //calculate volume of shpere
        volume=4.0/3*Math.PI*Math.pow(radius,3);
        System.out.println("volume of shpere ("+radius+") is " +volume);
    }//end of main method
}//end of class shpere