package sheridan;
public class Ch5Ex18P1
{
    public static void main(String args[])
    {
        int row , column;
        System.out.println("Pattern A");
        for(row=1;row<=6;row++)
        {
            for(column=1;column<=row;column++)
            {
                System.out.print(column);
            }
            System.out.println();
        }
    }
}