package application;

puclic class Vector2D
{
    private Double _x;
    private Double _y;
    
    //parameter consturctor
     public Vectoer2D(Double x,Double y)
     {
         this._x=x;
         this._y=y;
     }//end of constructor
     
     //method to calculate magnitude
     public Double magnitude()
     {
         return Math.sqrt(x*x + y*y);
     }//end of method magnitude
}//end of class