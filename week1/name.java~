import java.io.*;

public class name{
  
  public static void main(String[] args)throws IOException
  {
        InputStreamReader isr= new InputStreamReader(System.in);
        BufferedReader br= new BufferedReader(isr);
            String name;
                 System.out.println("whats your name??");
            name=br.readLine();
             
                   System.out.println("your name is:" + name);  
            
                   if(name.equalsIgnoreCase("akshay"))
                   {
                     System.out.println("Pet name is:Bhosdiwale Chacha");
                   }
                   else if(name.equalsIgnoreCase("rohit"))
                   {
                     System.out.println("Pet name is:MUNNA Bhaiya");
                   }  
                   else if(name.equalsIgnoreCase("ark"))
                   {
                     System.out.println("Pet name is:CHOTU");
                   }  
                   else
                   {
                     System.out.println("no pet name");
                   }  
                  
  } 
}